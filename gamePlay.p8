function _init()
	create_player()
	timer = 5
	timer_game = 5
	
	colors_square={
	64,68,72,76,128,132,136,
	140,192,196
	}
	
	random_color ={
	{196,196,196},
	{196,196,196},
	{196,196,196}
	}
	
	loop_end = false
	startgame = false
	scene="menu"
 x=50
 y=63
 perso_sel=0
 curseur = 1
 color_text = 0
end

function _update()

	if startgame == true then
		timer_color()
	end
	
	update_camera()
	check_flag()
	player_movement()
	starter()
	
	if scene == "menu" then
				update_menu()
	elseif scene == "perso" then
			update_perso()
	end
	
	if btnp(‚ùé) and x == 50 then
		perso_sel =1
		p.sprite = 37
	elseif btnp(‚ùé) and x == 60 then
		perso_sel =2
		p.sprite = 27
	elseif btnp(‚ùé) and x == 70 then
		perso_sel =3
		p.sprite = 48
	elseif btnp(‚ùé) and x == 80 then
		perso_sel =4
		p.sprite = 32
	end
	
	if timer <= 0  then
		timer_game -=0.03
	end
		
end

function _draw()
 cls()
	draw_map()
	create_plateform()
	
	rectfill(40,132,80,138,2)
	
	if timer >0 then
	color(7)
		print(timer,60,132)
	end
	
	if timer <= 0 then
		print_color()
	end
	
	if timer_game <= 0 then
		loop_end = false
		create_plateform()
		timer = 5
		timer_game = 5
	end
	
	print("time: ",40,132)
	draw_player()
	
	if scene=="menu" then
				draw_menu()
	elseif scene=="perso" then
				draw_perso()
	end
	
	if startgame == false then
		color(1)
		print("press üÖæÔ∏è to start", 30, 256-64)
	end
	
	if loop_end == true and timer == 0 then
	 random_color ={
	{196,196,196},
	{196,196,196},
	{196,196,196}
	}
	end
	
	starfield()
	
end
