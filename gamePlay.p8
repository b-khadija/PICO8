	spr(57,x,70)
	spr(37,50,60)
	spr(27,60,60)
	spr(48,70,60)
	spr(32,80,60)
	
	if perso_sel<1 then
		 curseur = 4
	elseif perso_sel>4 then
		 curseur = 1
	end
	
	if btnp(‚û°Ô∏è) then
		curseur+=1
	elseif btnp (‚¨ÖÔ∏è) then
		curseur-=1	
	end
	
	if x<50 then
		 x = 80
	elseif x>80 then
		 x = 50
	end
	
 if (btnp(‚û°Ô∏è)) x+=10
 if (btnp(‚¨ÖÔ∏è)) x-=10
		
end

