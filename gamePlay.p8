--player

function create_player()
	p={
		x=14,
		y=1,
		sprite=32,
		keys=0
		}
end

function draw_player()
	spr(p.sprite,p.x*8,p.y*8)
end

function player_movement()
	newx=p.x
	newy=p.y
	if (btnp(‚û°Ô∏è)) newx+=1
	if (btnp(‚¨ÖÔ∏è)) newx-=1
	if (btnp(‚¨áÔ∏è)) newy+=1
	if (btnp(‚¨ÜÔ∏è)) newy-=1
	
	interact(newx,newy) 

	if not check_flag(0,newx,newy) then
		p.x=mid(0,newx,127)
		p.y=mid(0,newy,63)
	end
end

function interact(x,y)
	if check_flag(1,x,y) then
		pick_up_key(x,y)
	elseif check_flag(2,x,y)
	and p.keys>=1 then
		open_door(x,y)
	end
end
